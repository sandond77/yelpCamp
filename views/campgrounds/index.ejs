<% layout('layouts/boilerplate') %>
    <h1> Campgrounds Page</h1>
    <div> <a href="/campgrounds/new">Add Campground</a> </div>
    <% for (let campground of campgrounds) {%>
        <div class="campground">
            <li>
                <a href="/campgrounds/<%=campground._id%>"><%= campground.title %> (<%= campground.location %>)</a>
            </li>
        </div>
    <% }%>
